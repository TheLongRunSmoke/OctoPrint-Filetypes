<form class="form-horizontal">
    <h3>Upload dialog default file types</h3>
    <div class="control-group">
        <div class="controls">
            <label class="checkbox">
                <input type="checkbox" data-bind="checked: settings.plugins.filetypes.stl" id="settings-stl"> stl
            </label>
        </div>
        <div class="controls">
            <label class="checkbox">
                <input type="checkbox" data-bind="checked: settings.plugins.filetypes.gcode" id="settings-gcode"> gcode
            </label>
        </div>
        <div class="controls">
            <label class="checkbox">
                <input type="checkbox" data-bind="checked: settings.plugins.filetypes.gco" id="settings-gco"> gco
            </label>
        </div>
        <div class="controls">
            <label class="checkbox">
                <input type="checkbox" data-bind="checked: settings.plugins.filetypes.g" id="settings-g"> g
            </label>
        </div>
    </div>
</form>
